@model Movie
@{
                        ViewData["Title"]="DisplayMovie2" ;
                        }
<h1></h1>

@foreach(var m in Model)
{
     <tr>
                <td>Id:</td>
                <td>
                    @Html.DisplayFor(m => m.ID)
                </td>
            </tr>
            <tr>

                <td>Title:</td>
                <td>
                    @Html.DisplayFor(m => m.Title)
                </td>
            </tr>
            <tr>
                <td>Genre:</td>
                <td>
                    @Html.DisplayFor(m => m.Genre)
                </td>
            </tr>
            <tr>

                <td>Year:</td>
                <td>
                    @Html.DisplayFor(m => m.Year)
                </td>
            </tr>
            <tr>
                <td>Actors:</td>
                <td>
                    @Html.DisplayFor(m => m.Actors)
                </td>
            </tr>
            <tr>
                <td>Directors:</td>
                <td>
                    @Html.DisplayFor(m => m.Directors)
                </td>
            </tr>
}





